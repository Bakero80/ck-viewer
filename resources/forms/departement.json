{
	"name": "departement.default",
	"description": "Fiche de description des Départements...",
	"window": {
		"title": "DEPARTEMENTS"
	},
	"dataUrl": "/packages/local/ck-viewer/resources/data/departement/{0}.json",
	"form": {
		"items": [{
			"fieldLabel": "ID Geofla",
			"name": "ID_GEOFLA",
			"prefix": "n° ",
			"suffix": " (validé)"
		},{
			"fieldLabel": "Nom",
			"name": "NOM_DEPT",
		  	"allowBlank" : false
		},{
			"xtype": "combo",
			"name": "NOM_REGION",

			"_store": [
				"un",
				"deux",
				"trois"
			],

		  	"store": "/packages/local/ck-viewer/resources/data/regions.json",
			"sorters": "NOM",

			"displayField": "NOM",
			"valueField": "NOM",
			"tpl": "<ul class='x-list-plain'><tpl for='.'><tpl if='hidden!=true'><li role='option' class='x-boundlist-item'>{NOM} ({CODE})</li></tpl></tpl></ul>"
		}]
	}
}